{
    "manifest_version": 2,
    "name": "Chord Namer",
    "description": "Enter 2 or 3 notes into the input boxes, click the button and find out what chord it is",
    "version": "1.0.0",
    "icons": { "128" : "g_icon_128.png"},
    "browser_action": {
        "default_icon": "g_icon.png",
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+Y",
            "mac": "Ctrl+Shift+L"
          }
        }
    }
}