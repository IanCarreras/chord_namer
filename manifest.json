{
    "manifest_version": 2,
    "name": "Chord Namer",
    "description": "Returns Chord Name",
    "version": "1.0.0",
    "icons": { "128" : "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "data.js",
            "controller.js",
            "background.js"
        ],
        "persistent": false
    },
    "permissions": ["activeTab"]
}